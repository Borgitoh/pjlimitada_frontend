{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pjlimitada": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pjlimitada",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [

              "src/assets/css/bootstrap.min.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/font/flaticon.css",
              "src/assets/css/jquery-ui.css",
              "src/assets/css/slick.css",
              "src/assets/css/slick-theme.css",
              "src/assets/css/animate.css",
              "src/assets/css/nice-select.css",
              "src/assets/css/jarallex.css",
              "src/assets/css/mapbox-gl.min.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "src/styles.scss"
            ],
            "scripts": [

              "src/assets/js/jquery-3.4.1.min.js",
               "src/assets/js/popper.min.js",
               "src/assets/js/bootstrap.min.js",
               "src/assets/js/jarallax.min.js",
               "src/assets/js/jarallax-video.min.js",
               "src/assets/js/slick.min.js",
               "src/assets/js/jquery-ui.js",
               "src/assets/js/isotope.pkgd.js",
               "src/assets/js/jquery.nice-select.min.js",
               "src/assets/js/jquery-steps.js",
               "src/assets/js/bootstrap-tagsinput.min.js",
               "src/assets/js/mapbox-gl.min.js",
               "src/assets/js/mapdata.js",
               "src/assets/js/wow.js",
               "src/assets/js/custom.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [ ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "allowedHosts": ["all"],
            "host": "0.0.0.0"
          },
          "configurations": {
            "production": {
              "browserTarget": "pjlimitada:build:production"
            },
            "development": {
              "browserTarget": "pjlimitada:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pjlimitada:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
